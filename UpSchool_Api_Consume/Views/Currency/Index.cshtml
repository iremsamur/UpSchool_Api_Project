@model IEnumerable<CurrencyListViewModel.Exchange_Rates>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<div class="container">
    <br />
    <table class="table table-bordered">
        <tr>
            <th>Döviz Kuru</th>
            <th>Değeri</th>
        </tr>
        @foreach(var item in Model){
            <tr>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
            </tr>
        }

    </table>
</div>


